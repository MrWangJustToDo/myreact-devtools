{
  "name": "@my-react-devtool/bridge",
  "description": "@my-react-devtool/bridge",
  "author": "MrWangJustToDo",
  "license": "MIT",
  "version": "0.0.1",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "type.js",
    "type.d.ts",
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/my-react-devtools"
  },
  "exports": {
    ".": "./index.js",
    "./type": "./type.js",
    "./package.json": "./package.json"
  },
  "buildOptions": [
    {
      "input": "./src/content/index.ts",
      "pkgName": "hook",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "iife/hook.js",
          "format": "iife",
          "type": true,
          "multiple": true
        }
      ]
    },
    {
      "input": "./src/content/index-forward.ts",
      "pkgName": "forward-dev",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "iife/forward-dev.js",
          "format": "iife",
          "type": true,
          "multiple": true
        }
      ]
    },
    {
      "input": "./src/popover/index.ts",
      "pkgName": "detector",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "iife/detector.js",
          "format": "iife",
          "type": true,
          "multiple": true
        }
      ]
    },
    {
      "input": "./src/background/proxy.ts",
      "pkgName": "proxy",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "iife/proxy.js",
          "format": "iife",
          "type": true,
          "multiple": true
        }
      ]
    },
    {
      "input": "./src/background/index.ts",
      "pkgName": "service-worker",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "iife/service-worker.js",
          "format": "iife",
          "type": true,
          "multiple": true
        }
      ]
    },
    {
      "input": "./src/panel/index.ts",
      "pkgName": "panel",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "iife/panel.js",
          "format": "iife",
          "type": true,
          "multiple": true
        }
      ]
    },
    {
      "input": "./src/type.ts",
      "pkgName": "type",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/type.js",
          "format": "cjs",
          "type": true
        }
      ]
    }
  ],
  "dependencies": {
    "@types/chrome": "^0.0.304"
  },
  "devDependencies": {
    "@my-react-devtool/core": "workspace:*",
    "@my-react/react": "^0.3.20",
    "@my-react/react-reconciler": "^0.3.20",
    "@my-react/react-shared": "^0.3.20",
    "@types/node": "^22.18.10",
    "reactivity-store": "^0.3.12",
    "socket.io-client": "^4.8.1"
  }
}
