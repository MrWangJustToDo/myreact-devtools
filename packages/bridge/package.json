{
  "name": "@my-react-devtool/bridge",
  "description": "@my-react-devtool/bridge",
  "author": "MrWangJustToDo",
  "license": "MIT",
  "version": "0.0.1",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/my-react-devtools"
  },
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "output": [
      {
        "dir": "./dist",
        "entryFileNames": "cjs/index.js",
        "format": "cjs",
        "type": true,
        "multiple": true,
        "sourcemap": true
      }
    ]
  },
  "dependencies": {
    "@types/chrome": "^0.0.268"
  },
  "devDependencies": {
    "@my-react-devtool/core": "workspace:*",
    "@my-react/react": "^0.3.1",
    "@my-react/react-dom": "^0.3.1",
    "@my-react/react-shared": "^0.3.1",
    "@my-react/react-reconciler": "^0.3.1"
  }
}
