{
  "name": "@my-react-devtool/core",
  "description": "@my-react-devtool/core",
  "author": "MrWangJustToDo",
  "license": "MIT",
  "version": "0.0.1",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "event.js",
    "event.d.ts",
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWangJustToDo/my-react-devtools"
  },
  "exports": {
    ".": "./index.js",
    "./event": "./event.js",
    "./package.json": "./package.json"
  },
  "buildOptions": [
    {
      "input": "./src/index.ts",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/index.js",
          "format": "cjs",
          "type": true,
          "multiple": true
        }
      ]
    },
    {
      "input": "./src/event.ts",
      "output": [
        {
          "dir": "./dist",
          "entryFileNames": "cjs/event.js",
          "format": "cjs"
        }
      ]
    }
  ],
  "devDependencies": {
    "@my-react/react": "^0.3.14",
    "@my-react/react-reconciler": "^0.3.14",
    "@my-react/react-shared": "^0.3.14",
    "@types/lodash": "^4.17.15"
  },
  "dependencies": {
    "error-stack-parser": "^2.1.4",
    "lodash": "^4.17.21"
  }
}
